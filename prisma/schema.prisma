generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model chart_data {
  id    Int      @id @default(autoincrement())
  value Int
  sex   Boolean?
  date  DateTime @db.Timestamp(6)
}
